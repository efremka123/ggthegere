                                          theLeftOption = vdata.option3;
                                      } else if (optionsArr1.indexOf(selectedArrNotEmpty[0]) > -1 && optionsArr3.indexOf(selectedArrNotEmpty[1]) > -1
                                                 || optionsArr1.indexOf(selectedArrNotEmpty[1]) > -1 && optionsArr3.indexOf(selectedArrNotEmpty[0]) > -1) {
                                         theLeftOption = vdata.option2 ;
                                      } else if (optionsArr2.indexOf(selectedArrNotEmpty[0]) > -1 && optionsArr3.indexOf(selectedArrNotEmpty[1]) > -1||
                                                optionsArr2.indexOf(selectedArrNotEmpty[1]) > -1 && optionsArr3.indexOf(selectedArrNotEmpty[0]) > -1) {
                                         theLeftOption = vdata.option1 ;
                                      }
                                      if(theLeftOption != '' && (
                                          vdata.option1 == selectedArrNotEmpty[0] && vdata.option2 == selectedArrNotEmpty[1] || vdata.option1 == selectedArrNotEmpty[1] && vdata.option2 == selectedArrNotEmpty[0]
                                       || vdata.option1 == selectedArrNotEmpty[0] && vdata.option3 == selectedArrNotEmpty[1] || vdata.option1 == selectedArrNotEmpty[1] && vdata.option3 == selectedArrNotEmpty[0]
                                       || vdata.option2 == selectedArrNotEmpty[0] && vdata.option3 == selectedArrNotEmpty[1] || vdata.option2 == selectedArrNotEmpty[1] && vdata.option3 == selectedArrNotEmpty[0]
                                         )) {
                                          var inputObj  = document.querySelector('variant-radios input[value="' + theLeftOption + '"]');
                                          var labelObj = document.querySelector('variant-radios label[for="' + inputObj.id + '"]');
                                          if (!vdata.available) {
                                            labelObj.classList.add('soldout');
                                          }else {
                                            labelObj.classList.remove('soldout');
                                          }
                                      }
                                  } 
                                  if (vdata.available == false) {
                                    //vLabel.classList.add('soldout');
                                  }
                               }
                           }
                           //All 2 or 3 Options are selected
                           if (fieldsets.length - selectedArrNotEmpty.length == 0) {
                               if (fieldsets.length == 2) {//if 2 options
                                  for(var vindex in variantData) {
                                    var vdata = variantData[vindex];
                                    var theLeftOption = '';
                                    if (vdata.option1 == selectedArrNotEmpty[0]) {
                                        theLeftOption = vdata.option2;
                                    } else if (vdata.option2 == selectedArrNotEmpty[0]) {
                                         theLeftOption = vdata.option1;
                                    } 
                                    if (theLeftOption) {
                                        var inputObj  = document.querySelector('variant-radios input[value="' + theLeftOption + '"]');
                                        var labelObj = document.querySelector('variant-radios label[for="' + inputObj.id + '"]');
                                        if (!vdata.available) {
                                            labelObj.classList.add('soldout');
                                        } else {
                                            labelObj.classList.remove('soldout');
                                        }
                                    }
                                  }
                               } else if (fieldsets.length == 3) {//if 3 options
                                  var _val = this.value;
                                  selectedArrNotEmpty = selectedArrNotEmpty.reverse();
                                  for(var vindex in variantData) {
                                    var vdata = variantData[vindex];
                                    var theLeftOption = '';
                                    if (vdata.option1 == selectedArrNotEmpty[0] && vdata.option2 == selectedArrNotEmpty[1]) {
                                          theLeftOption = vdata.option3;
                                    } else if ( vdata.option2 == selectedArrNotEmpty[2] && vdata.option3 == selectedArrNotEmpty[3]) {
                                          theLeftOption = vdata.option1;
                                    } else if (vdata.option1 == selectedArrNotEmpty[1] && vdata.option3 == selectedArrNotEmpty[3]) {
                                          theLeftOption = vdata.option2;
                                    }
                                    if (theLeftOption) {
                                        var inputObj  = document.querySelector('variant-radios input[value="' + theLeftOption + '"]');
                                        var labelObj = document.querySelector('variant-radios label[for="' + inputObj.id + '"]');
                                        if (!vdata.available) {
                                            labelObj.classList.add('soldout');
                                        } else {
                                            labelObj.classList.remove('soldout');
                                        }
                                    }
                                  }
                               }
                           }
                           if (selectedArrNotEmpty.length == fieldsets.length) {
                                var myEvent = new Event('click');
                                document.querySelector('#ModalClose-variant_picker').dispatchEvent(myEvent);
                           }
                        });
                        //var myEvent = new Event('change');
                        //radioInput.dispatchEvent(myEvent);
                    });
                   
               }
                setTimeout(function() {
                    // IE
                    if(document.all) {
                       for (var i = 0; i < fieldsets.length-1; i++){
                           var fsLabels = fieldsets[i].querySelectorAll('label');
                            fsLabels[0].click();
                       }
                    } else {  // others
                        for (var i = 0; i < fieldsets.length-1; i++){
                           //var myEvent = new MouseEvent('click');
                           var fsLabels = fieldsets[i].querySelectorAll('label');
                           //fsLabels[0].dispatchEvent(myEvent);
                           var e = document.createEvent("MouseEvents");
                           e.initEvent("click", true, true);
                           fsLabels[0].dispatchEvent(e);
                        }
                    }
                }, 1000);
           }
       }
      
    });
  </script><script type="application/ld+json">
