<script >
  var close = document.querySelector('.close_tipbar');
    close.onclick = function(){
        this.parentElement.remove();
    }
    //https://www.poizon.us/?utm_source=push&utm_medium=dewupush&utm_id=dewuipphone
    //https://www.poizon.us/?utm_source=push&utm_medium=dewuxiaoxibox&utm_id=dewuipphone
    //https://www.poizon.us/?utm_source=dewupush0927&utm_medium=dewupush0927
    //https://www.poizon.us/?utm_source=dewuxiaoxibox0927&utm_medium=dewuxiaoxibox0927
    /** dewupush  dewuxiaoxibox  **/
    var _value = GetURLParameter('utm_medium');
    
    if (_value && (_value.indexOf('dewupush') == 0 || _value.indexOf('dewuxiaoxibox') == 0 )) {
      var top_tip_bar = document.querySelector('.top_tip_bar');
      top_tip_bar.style.display='block';
      top_tip_bar.eq(0).style.display='block';
    }
    function GetURLParameter(sParam){
      var sPageURL = window.location.search.substring(1);
      var sURLVariables = sPageURL.split('&');
      for (var i = 0; i < sURLVariables.length; i++){
          var sParameterName = sURLVariables[i].split('=');
          if (sParameterName[0] == sParam){
              return sParameterName[1];
          }
      }
  }
</script>

