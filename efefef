      var addViewProductsToWishList = document.querySelectorAll('.product__info-container .swym-add-to-wishlist');
      //var addViewProductsToWishList = document.querySelectorAll('.swym-add-to-wishlist-view-product');
      if (addViewProductsToWishList && addViewProductsToWishList != null ){
           window._swat.evtLayer.addEventListener( window._swat.JSEvents.addedToWishlist, function(eventData) {
               var data = {};
                   data.sku = '';
                   data.vendor = '';
                   data.type = '';
                   data.price = parseFloat( eventData.detail.d.pr);
                   data.title = eventData.detail.d.dt;
                   data.variant_id = eventData.detail.d.epi;
                   data.product_id = eventData.detail.d.empi;
                   data.vtitle = eventData.detail.d.vi;
               var variant_id = eventData.detail.d.epi;
               if (productObj) {
                   var productVariants = productObj.variants;
                   var selectedVariant = {};
                   for (let i = 0; i < productVariants.length; i++) {
                       if (variant_id == productVariants[i].id) {
                           selectedVariant = productVariants[i];
                       }
                   }
                   data.sku = selectedVariant.sku;
                   data.vendor = productObj.vendor;
                   data.type = productObj.type;
               }
               var cardObj = document.querySelector('.card-wrapper[data-product-id="' + data.product_id + '"]');
               if (cardObj) {
                   data.sku = cardObj.getAttribute('data-product-sku');
               }
               window.addToWishlistHandle(data);
           });
       }
  }
  
});


</script>


