<script type="text/javascript">

window.dataLayer = window.dataLayer || [];window.dataLayer.push({  
      'login_status': 'not-logged'
        });   
  
  
window.appStart = function(){

  window.productPageHandle = function(){
    var productName = "The North Face Novelty GTX Paisley Mountain Jacket Black";
    var productId = "7939992912091";
    var productPrice = "181.00";
    var productBrand = "POIZONGLOBAL";
    var productCollection = "All Products";
    var productType = "Jackets";
    var productSku = "604786910";
    var productVariantId = "43869277159643";
    var productVariantTitle = "M / Black";
    window.dataLayer.push({
      event: "analyzify_productDetail",
      productId: productId,
      productName: productName,
      productPrice: productPrice,
      productBrand: productBrand,
      productCategory: productCollection,
      productType: productType,
      productSku: productSku,
      productVariantId: productVariantId,
      productVariantTitle: productVariantTitle,
      currency: "USD",
    });
  };


  window.allPageHandle = function(){
    window.dataLayer.push({
      event: "ga4kit_info",
      contentGroup: "product",
      
        userType: "visitor",
      
    });
  };


  window.addToCartHandle = function(currentItem){
      console.log(currentItem);
      var totalPrice = parseFloat((currentItem.final_line_price/100).toFixed(2));
     
      var cartItemsName = [];
      var cartItemsId = [];
      var cartItemsCategory = [];
      var cartItemsBrand = [];
      var cartItemsType = [];
      var cartItemsPrice = [];
      var cartItemsSku = [];
      var cartItemsvariantId = [];
      var cartItemsQuantity = [];
      var cartItemsvariantTitle = [];
      var totalQuantity = 0;
    
      if (currentItem) {
          cartItemsName.push( currentItem.product_title);
          cartItemsId.push( currentItem.product_id );
          cartItemsPrice.push(currentItem.price);
          cartItemsSku.push(currentItem.sku);
          cartItemsQuantity.push(currentItem.quantity);
          cartItemsvariantId.push(currentItem.variant_id);
          cartItemsvariantTitle.push(currentItem.variant_title);
          cartItemsCategory.push("--");
          cartItemsBrand.push(currentItem.vendor);
          cartItemsType.push(currentItem.product_type);
          totalQuantity += currentItem.quantity;
      }
      window.dataLayer.push({
          event: "shopify_addToCart",
          currency: "USD",
          totalValue: totalPrice,
          productName: cartItemsName,
          productId: cartItemsId,
          productBrand: cartItemsBrand,
          productCategory: cartItemsCategory,
          productVariantId: cartItemsvariantId,
          productVariantTitle: cartItemsvariantTitle,
          productSku: cartItemsSku,
          productType: cartItemsType,
          productPrice: cartItemsPrice,
          productQuantity: cartItemsQuantity,
      });
  };




  
    
  window.searchHandle = function(searchTerm){
      window.dataLayer.push({
          event: "shopify_search",
          search_term: searchTerm,
      });
  };

  

  window.beginCheckoutHandle = function() {
      var totalPrice = "0.00".replace(",", ".");
      var cartItemsName = [];
      var cartItemsId = [];
      var cartItemsCategory = [];
      var cartItemsBrand = [];
      var cartItemsType = [];
      var cartItemsPrice = [];
      var cartItemsSku = [];
      var cartItemsvariantId = [];
      var cartItemsQuantity = [];
      var cartItemsvariantTitle = [];
      var totalQuantity = 0;
console.log(window.Shopify.routes.root + 'cart.js');
      fetch(window.Shopify.routes.root + 'cart.js')
      .then((response) => response.json())
      .then(response => {
          var cartItems = response.items;
          console.log(response);
          totalQuantity = response.item_count;
          totalPrice = response.total_price/100;
          if (totalQuantity > 0) {
             cartItems.forEach((cartItem) => {
                cartItemsName.push(cartItem.title);
                cartItemsId.push(cartItem.id);
                cartItemsPrice.push(cartItem.price);
                cartItemsSku.push(cartItem.sku);
                cartItemsQuantity.push(cartItem.quantity);
                cartItemsvariantId.push(cartItem.variant_id);
                cartItemsvariantTitle.push(cartItem.variant_title);
                cartItemsType.push(cartItem.product_type);
             });
            window.dataLayer.push({
                event: "shopify_beginCheckout",
                currency: "USD",
                totalValue: response.total_price,
                productName: cartItemsName,
                productId: cartItemsId,
                productVariantId: cartItemsvariantId,
                productVariantTitle: cartItemsvariantTitle,
                productSku: cartItemsSku,
                productType: cartItemsType,
                productPrice: cartItemsPrice,
                productQuantity: cartItemsQuantity,
            });
          }
      }).catch((error) => {
        console.error('Error:', error);
      });
  };


    window.addToWishlistHandle = function(data){
      var itemsName = [];
      var itemsId = [];
      var itemsCategory = [];
      var itemsBrand = [];
      var itemsType = [];
      var itemsPrice = [];
      var itemsSku = [];
      var itemsvariantId = [];
      var itemsQuantity = [];
      var itemsvariantTitle = [];
      var totalQuantity = 0;

      if (data) {
          totalPrice = data.price ;
          itemsName.push( data.title);
          itemsId.push( data.product_id );
          itemsPrice.push(data.price );
          itemsSku.push(data.sku);
          itemsQuantity.push(1);
          itemsvariantId.push( data.variant_id);
          itemsvariantTitle.push(data.vtitle );
          itemsCategory.push("--");
          itemsBrand.push(data.vendor);
          itemsType.push(data.type);
          totalQuantity += 1;
      }

    console.log({
          event: "Shopify_addToWishlist",
          currency: "USD",
          totalValue: totalPrice,
          productName: itemsName,
          productId: itemsId,
          productBrand: itemsBrand,
          productCategory: itemsCategory,
          productVariantId: itemsvariantId,
          productVariantTitle: itemsvariantTitle,
          productSku: itemsSku,
          productType: itemsType,
          productPrice: itemsPrice,
          productQuantity: itemsQuantity,
      });

      window.dataLayer.push({
          event: "Shopify_addToWishlist",
          currency: "USD",
          totalValue: totalPrice,
          productName: itemsName,
          productId: itemsId,
          productBrand: itemsBrand,
          productCategory: itemsCategory,
          productVariantId: itemsvariantId,
          productVariantTitle: itemsvariantTitle,
          productSku: itemsSku,
          productType: itemsType,
          productPrice: itemsPrice,
          productQuantity: itemsQuantity,
      });
  };
  
  allPageHandle();
  
    productPageHandle();
  

}
appStart();
