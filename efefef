<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function() {
     setTimeout(function(){ 
          var htmlDom = document.getElementsByTagName('html')[0];
          htmlDom.classList.remove('gf-top_one');
          htmlDom.classList.add('gf-left');
     }, 2000);
    if (window.matchMedia('(min-width: 750px)').matches) {
      var bannerBox = document.querySelector('.grid__item .banner__box');
      var slideObject = document.querySelector('.slider.slider--everywhere .slider__slide');
      var topToSet = ( slideObject.offsetHeight - bannerBox.offsetHeight) / 2;
      bannerBox.style.top = topToSet + 'px';
    }
});
</script>

    
